local Window = OrionLib:MakeWindow({Name = "Mthhx Hub (BETA)", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local function AutoClick()
  while getgenv().AutoClick == true do task.wait()
    local RS = game:GetService("ReplicatedStorage")
    local Remote = RS:WaitForChild("Remotes", 9e9)
    Remote:WaitForChild("ParryButtonPress"):Fire()
  end
end

