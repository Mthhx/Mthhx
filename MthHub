-- DEFINE LIBRARY
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

--DEFINE WINDOW
local Window = OrionLib:MakeWindow({Name = "Mth Hub KEY System", HidePremium = false, SaveConfig = false, ConfigFolder = "Mth Hub KEY System", IntroEnabled = false})

-- DEFINE TABS
local Tab = Window:MakeTab({
    Name = "Key System",
    Icon = " ",
    PremiumOnly = false
})

-- DEFINE KEYSYSTEM TAB
local Section = Tab:AddSection({
    Name = "Key System"
})

_G.Key = "Mth_Hub"
_G.KeyInput = "string"

function CorrectKey()
     loadstring(game:HttpGet(('https://raw.githubusercontent.com/Mthhx/Mthhx/main/Mth%20Hub/Blade%20ball'),true))()
end

function CorrectKeyMessage()
    OrionLib:MakeNotification({
    Name = "Key System",
    Content = "You've entered the correct key!",
    Image = " ",
    Time = 4
    })
    wait(1)
    OrionLib:Destroy()
end

function WrongKeyMessage()
    OrionLib:MakeNotification({
    Name = "Key System",
    Content = "Key incorreta ",
    Image = " ",
    Time = 4
    })
end

Tab:AddTextbox({
    Name = "Enter Key",
    Default = "",
    TextDisappear = false,
    Callback = function(keycode)
        _G.KeyInput = keycode
    end      
})

Tab:AddButton({
    Name = "Check Key",
    Callback = function()
          if _G.KeyInput == _G.Key then
               CorrectKeyMessage()
               CorrectKey()
          else
               WrongKeyMessage()
          end
    end    
})


Tab:AddButton({
  Name = "Click To Get Key in Discord",
  Callback = function()
    setclipboard("https://discord.com/invite/XF9zAfrDne")
  end
})

OrionLib:Init()
